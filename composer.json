{
    "name": "innmind/cron",
    "type": "library",
    "description": "Helper to install crontabs on a machine",
    "keywords": ["cron", "crontab", "job"],
    "homepage": "http://github.com/Innmind/Cron",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/Cron/issues"
    },
    "require": {
        "php": "~8.1",
        "innmind/immutable": "~4.2",
        "innmind/server-control": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Cron\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Cron\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "vimeo/psalm": "~4.4",
        "innmind/black-box": "^4.16",
        "innmind/coding-standard": "~2.0"
    },
    "suggest": {
        "innmind/operating-system": "To easily access to a machine control mechanism"
    }
}
